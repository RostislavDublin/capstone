# Production Environment Variables
# These are passed to the deployed agent in Cloud Run
# Copy to .env.production and configure before deployment

# Use Vertex AI backend
GOOGLE_GENAI_USE_VERTEXAI="1"

# Enable Agent Engine telemetry
GOOGLE_CLOUD_AGENT_ENGINE_ENABLE_TELEMETRY="true"

# Vertex AI location (must match deployment region)
VERTEX_LOCATION="us-west1"

# Firestore configuration
FIRESTORE_DATABASE="(default)"
FIRESTORE_COLLECTION_PREFIX="prod"

# Model Configuration
ANALYZER_MODEL="gemini-2.5-flash"
CONTEXT_MODEL="gemini-2.5-pro"
REPORTER_MODEL="gemini-2.5-flash"

# Memory Bank
MEMORY_BANK_ENABLED="true"
SESSION_TIMEOUT="3600"

# Logging
LOG_LEVEL="INFO"
ENABLE_TRACING="true"
