# Deployment Configuration for Vertex AI Agent Engine
# Copy to .env.deploy and fill in your values

# GCP Project Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1

# Authentication
# Path to service account key JSON file
GOOGLE_APPLICATION_CREDENTIALS=./deployment/service-account-key.json

# Deployment Settings
AGENT_ENGINE_NAME=code-review-orchestrator
AUTH_MODE=service_account

# GitHub Webhook Configuration (for production)
WEBHOOK_SECRET=your_webhook_secret_here
GITHUB_APP_ID=your_github_app_id  # If using GitHub App
GITHUB_APP_PRIVATE_KEY_PATH=./deployment/github-app-key.pem

# Resource Configuration
ENABLE_AUTO_SCALING=true
MIN_INSTANCES=0
MAX_INSTANCES=10
