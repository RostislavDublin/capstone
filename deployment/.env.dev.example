# Local Development Configuration
# Copy this to .env.dev and fill in your values

# Gemini API Key (get from https://aistudio.google.com/apikey)
GOOGLE_API_KEY=your_gemini_api_key_here

# GitHub Token (get from https://github.com/settings/tokens)
# Required scopes: repo, read:org
GITHUB_TOKEN=your_github_token_here

# Webhook secret (generate random string)
GITHUB_WEBHOOK_SECRET=your_webhook_secret_here

# Test fixture repository (managed programmatically)
TEST_FIXTURE_REPO=RostislavDublin/code-review-test-fixture
AUTO_DEPLOY_FIXTURE=false
