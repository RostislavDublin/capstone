# Local Development Configuration
# Copy to .env and fill in your values

# GitHub Token (for repository access)
# Get your token at: https://github.com/settings/tokens
GITHUB_TOKEN="your-github-token"

# Test Repository (for demos and integration tests)
TEST_REPO_NAME="quality-guardian-test-fixture"

# Google Cloud Project (Vertex AI)
GOOGLE_CLOUD_PROJECT="your-gcp-project-id"
VERTEX_LOCATION="us-west1"
GOOGLE_CLOUD_LOCATION="us-west1"

# Use Vertex AI backend (not AI Studio)
GOOGLE_GENAI_USE_VERTEXAI="1"

# Service Account Credentials (points to local file)
GOOGLE_APPLICATION_CREDENTIALS="./service-account-key.json"

# Model Configuration
ANALYZER_MODEL="gemini-2.5-flash"
CONTEXT_MODEL="gemini-2.5-pro"
REPORTER_MODEL="gemini-2.5-flash"

# Memory Bank
MEMORY_BANK_ENABLED="true"
SESSION_TIMEOUT="3600"

# Firestore Configuration (local development)
FIRESTORE_DATABASE="(default)"
FIRESTORE_COLLECTION_PREFIX="dev"

# Logging
LOG_LEVEL="INFO"
ENABLE_TRACING="true"
